@using CrudMVCRazorFetch.Models.ViewModel
@model List<ListPeopleViewModel>
@{
    Layout = null;
}

@foreach (var oPeople in Model)
{
    <div>
        Nombre: @oPeople.Name<br />
        Edat: @oPeople.Age<br />
        <input onclick="document.location.href='@Url.Content("~/People/Edit/"+oPeople.Id)'" type="button" value="Editar" />
        <input onclick="jsDelete(@oPeople.Id)" type="button" value="Eliminar" />
    </div>
}

